<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    style="@style/NoActionBar"
    android:theme="@style/NoActionBarTheme"
    android:orientation="vertical">
    <include layout="@layout/tool_bar_11c"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">

        <LinearLayout
            android:id="@+id/llScroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <View
                    android:id="@+id/divider1"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/rect6g" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/shape_rect14"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/sky_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clipToPadding="false"
                        android:layout_alignParentTop="true"
                        android:gravity="center_vertical"
                        android:background="@drawable/rect_their4b"
                        android:orientation="horizontal"
                        android:paddingEnd="1dp"
                        android:weightSum="1"
                        android:paddingLeft="1dp"
                        android:paddingRight="1dp"
                        android:paddingStart="1dp">

                        <TextView
                            android:id="@+id/sky_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:elevation="2dp"
                            android:layout_weight="1"
                            android:text="Skylight Count Updates for Date"
                            android:gravity="center"
                            android:textAlignment="center"
                            android:textStyle="italic"
                            android:paddingEnd="10dp"
                            android:padding="20dp"
                            android:paddingLeft="5dp"
                            android:paddingRight="5dp"
                            android:paddingStart="5dp"
                            android:layout_marginEnd="10dp"
                            android:singleLine="true"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="22sp" />

                    </LinearLayout>


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        app:cardCornerRadius="2dp"
                        app:cardElevation="2dp"
                        android:layout_below="@id/sky_1"
                        android:id="@+id/card_date"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        app:cardPreventCornerOverlap="false"
                        app:contentPadding="0dp">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:padding="20dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/rect6g"
                            android:layout_height="wrap_content">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/tx_text_date"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:textSize="15sp"
                                android:focusable="false"
                                android:drawableStart="@drawable/ic_icon_foreground"
                                android:textColor="@color/black"
                                android:layout_gravity="center_vertical|left"
                                android:gravity="center_vertical|left"
                                android:background="@null"
                                android:text="Select Date of Counts" />
                            <DatePicker
                                android:id="@+id/count_date_Super"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:textSize="15sp"
                                android:focusable="false"
                                android:inputType="date"
                                android:backgroundTintMode="src_in"
                                android:drawableStart="@drawable/ic_icon_foreground"
                                android:datePickerMode="spinner"
                                android:spinnersShown="true"
                                android:yearListSelectorColor="@color/white"
                                android:calendarTextColor="@color/white"
                                android:layout_gravity="center_vertical|left"
                                android:gravity="center_vertical|left"
                                android:calendarViewShown="false" />

                        </androidx.appcompat.widget.LinearLayoutCompat>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="70dp"
                        app:cardCornerRadius="2dp"
                        app:cardElevation="2dp"
                        android:gravity="end"
                        android:id="@+id/card_Date5"
                        app:cardBackgroundColor="@color/white"
                        android:layout_marginTop="10dp"
                        android:layout_gravity="end"
                        android:layout_below="@id/card_date"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        app:cardPreventCornerOverlap="false"
                        app:contentPadding="0dp">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:weightSum="1"
                            android:layout_height="wrap_content">
                            <androidx.appcompat.widget.AppCompatButton
                                android:layout_width="match_parent"
                                android:text="Search Database"
                                android:textSize="17sp"
                                android:padding="20dp"
                                android:layout_weight="1"
                                android:layout_marginStart="20dp"
                                android:layout_marginEnd="20dp"
                                android:textStyle="bold"
                                android:id="@+id/btnSearchDBC"
                                android:textColor="@android:color/holo_red_dark"
                                android:background="@drawable/rect_their_message"
                                android:gravity="center_horizontal"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                    </androidx.cardview.widget.CardView>

                    <LinearLayout
                        android:id="@+id/layout2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_below="@id/card_Date5"
                        android:paddingEnd="1dp"
                        android:layout_marginBottom="10dp"
                        android:paddingLeft="1dp"
                        android:paddingRight="1dp"
                        android:paddingStart="1dp">

                        <TextView
                            android:id="@+id/CustomerPaymentToday22"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:layout_marginBottom="10dp"
                            android:text="Customer Payment Today N:"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/CustomerSelect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:singleLine="true"
                            android:layout_marginTop="10dp"
                            android:gravity="center_horizontal"
                            android:layout_marginBottom="10dp"
                            android:text="Select Customer"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textAlignment="center"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="20sp"
                            android:textStyle="bold" />

                        <Spinner
                            android:layout_width="match_parent"
                            android:textColorHint="@color/colorPrimaryDark"
                            android:id="@+id/spnCusPayment"
                            android:padding="30dp"
                            android:background="@drawable/shape_rect14a"
                            android:textAlignment="center"
                            android:layout_marginBottom="10dp"
                            android:layout_gravity="center_horizontal"
                            android:gravity="center_horizontal"
                            android:layout_height="wrap_content"/>

                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/buttonCusPayment"
                            android:layout_width="match_parent"
                            android:padding="20dp"
                            android:background="@drawable/shape_rect_6"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:onClick="getCusPaymentToday"
                            android:text="Get Customer Details Today"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layout4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_below="@id/layout2"
                        android:paddingEnd="1dp"
                        android:layout_marginBottom="10dp"
                        android:paddingLeft="1dp"
                        android:paddingRight="1dp"
                        android:paddingStart="1dp">

                        <TextView
                            android:id="@+id/TellerTotalPayment33"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:text="Teller Total Payment N:"
                            android:layout_marginStart="5dp"
                            android:layout_marginBottom="10dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />
                        <TextView
                            android:id="@+id/TellerNewCus33"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:text="Teller New Cus"
                            android:layout_marginBottom="10dp"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/txtSelectTeller"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:singleLine="true"
                            android:layout_marginTop="10dp"
                            android:gravity="center_horizontal"
                            android:layout_marginBottom="10dp"
                            android:text="Select Teller"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textAlignment="center"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="20sp"
                            android:textStyle="bold" />

                        <Spinner
                            android:layout_width="match_parent"
                            android:textColorHint="@color/colorPrimaryDark"
                            android:id="@+id/spnTellerPayment"
                            android:padding="30dp"
                            android:background="@drawable/shape_rect14a"
                            android:textAlignment="center"
                            android:layout_marginBottom="10dp"
                            android:layout_gravity="center_horizontal"
                            android:gravity="center_horizontal"
                            android:layout_height="wrap_content"/>

                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/buttonPaymentTellerT"
                            android:layout_width="match_parent"
                            android:padding="20dp"
                            android:background="@drawable/shape_rect_6"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:onClick="getTellerTotalPayment"
                            android:text="Get Teller Details Today "
                            android:layout_weight="1"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/layout5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_below="@id/layout4"
                        android:paddingEnd="1dp"
                        android:layout_marginBottom="10dp"
                        android:paddingLeft="1dp"
                        android:paddingRight="1dp"
                        android:paddingStart="1dp">
                        <TextView
                            android:id="@+id/BranchPaymentToday33"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:text="Branch Payment Today N:"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/BranchTotalPayment33"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:layout_marginBottom="10dp"
                            android:text="Branch Total Payment N:"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />
                        <TextView
                            android:id="@+id/CustomersforBranch222"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:text="Customers for Branch:"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/txtSelectBranch"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:singleLine="true"
                            android:layout_marginTop="10dp"
                            android:gravity="center_horizontal"
                            android:layout_marginBottom="10dp"
                            android:text="Select Branch"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textAlignment="center"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="20sp"
                            android:textStyle="bold" />

                        <Spinner
                            android:layout_width="match_parent"
                            android:textColorHint="@color/colorPrimaryDark"
                            android:id="@+id/spnBranchT"
                            android:padding="30dp"
                            android:background="@drawable/shape_rect14a"
                            android:textAlignment="center"
                            android:layout_marginBottom="10dp"
                            android:layout_gravity="center_horizontal"
                            android:gravity="center_horizontal"
                            android:layout_height="wrap_content"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/btnPBranchT"
                            android:layout_width="match_parent"
                            android:padding="20dp"
                            android:background="@drawable/shape_rect_6"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:singleLine="true"
                            android:onClick="getTotalPaymentBranch"
                            android:text="Get Branch Details Today"
                            android:layout_weight="2"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                    </LinearLayout>


                </RelativeLayout>


                <View
                    android:id="@+id/divider"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="#1effffff" />

            </androidx.cardview.widget.CardView>

            <ImageView
                android:id="@+id/iv"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/awajima" />


        </LinearLayout>

    </ScrollView>

</LinearLayout>